--out table_visible_off         --spritesheet --button --images bg -tint midi_table_button_bg_off      visible     -tint midi_table_button_symbol_off
--out table_mute_off            --spritesheet --button --images bg -tint midi_table_button_bg_off      mute        -tint midi_table_button_symbol_off
--out table_solo_off            --spritesheet --button --images bg -tint midi_table_button_bg_off      solo        -tint midi_table_button_symbol_off
--out table_recarm_off          --spritesheet --button --images bg -tint midi_table_button_bg_off      arm         -tint midi_table_button_symbol_off
--out table_remove_off          --spritesheet --button --images bg -tint midi_table_button_bg_off      remove      -tint midi_table_button_symbol_off

