--clear-output-folder

#       Monitor FX
# --------------------------------------

# main tab
--out monitor_fx_byp        --spritesheet --button --images mon_fill        -tint color_gray        mon_text            -tint button_symbol_on      mon_border      -tint button_border
--out monitor_fx_off        --spritesheet --button --images mon_fill        -tint button_bg_off     mon_text            -tint button_symbol_off     mon_border      -tint button_border
--out monitor_fx_on         --spritesheet --button --images mon_fill        -tint color_cyan        mon_text            -tint button_symbol_on      mon_border      -tint button_border

# bypass
--out monitor_fx_byp_byp    --spritesheet --button --images mon_fx_fill     -tint color_gray        mon_fx_bypassed     -tint button_symbol_on      mon_fx_border   -tint button_border
--out monitor_fx_byp_off    --spritesheet --button --images mon_fx_fill     -tint button_bg_off     mon_fx_on           -tint button_symbol_off     mon_fx_border   -tint button_border
--out monitor_fx_byp_on     --spritesheet --button --images mon_fx_fill     -tint color_cyan        mon_fx_on           -tint button_symbol_on      mon_fx_border   -tint button_border

#       Tab
# --------------------------------------

--out tab_down      tab_up        --single-level 100    --frame frame_tab   --images    tab_fill -tint gui_tab_off
--out tab_down_sel  tab_up_sel    --single-level 100    --frame frame_tab   --images    tab_fill -tint gui_bg

#       Scrollbar
# --------------------------------------

# mixer scrollbar
--out scrollbar --single-level 100                                                                                                                                                              \
                --images scrollbar_bg   -tint gui_scrollbar_bg                                                                                                                                  \
                scrollbar_bars          -tint gui_scrollbar_bar         scrollbar_bars_hover        -tint gui_scrollbar_bar         scrollbar_bars_click        -tint gui_scrollbar_bar         \
                scrollbar_bars_hover    -tint overlay_hover             scrollbar_bars_click        -tint overlay_click                                                                         \
                scrollbar_symbols       -tint gui_scrollbar_symbols     scrollbar_symbols_hover     -tint gui_scrollbar_symbols     scrollbar_symbols_click     -tint gui_scrollbar_symbols     \
                scrollbar_symbols_hover -tint overlay_hover             scrollbar_symbols_click     -tint overlay_click                                                                         \
                frame/frame_scrollbar

# tcp scrollbar
--copy scrollbar scrollbar_2

# window scrollbar
