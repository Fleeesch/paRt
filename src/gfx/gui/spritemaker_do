                --images scrollbar_bg   -tint gui_scrollbar_window_bg                                                                                                                                           \
                scrollbar_bars          -tint gui_scrollbar_window_bar  scrollbar_bars_hover            -tint gui_scrollbar_window_bar          scrollbar_bars_click        -tint gui_scrollbar_window_bar      \
                scrollbar_bars_hover    -tint overlay_hover scrollbar_bars_click                        -tint overlay_click                                                                                     \
                scrollbar_symbols       -tint gui_scrollbar_window_symbols  scrollbar_symbols_hover     -tint gui_scrollbar_window_symbols      scrollbar_symbols_click     -tint gui_scrollbar_window_symbols  \
                scrollbar_symbols_hover -tint overlay_hover  scrollbar_symbols_click                    -tint overlay_click                                                                                     \
                frame/frame_scrollbar            

