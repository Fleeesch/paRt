# phase
--out gen_phase_norm    --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint generic_button_bg_off       phase       -tint generic_button_symbol_off     -flip-v -alpha 0.25     phase   -tint generic_button_symbol_off                 border_small -tint generic_button_border
--out gen_phase_inv     --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_cyan                  phase       -tint generic_button_symbol_on      -flip-v                 phase   -tint generic_button_symbol_on  -alpha 0.25     border_small -tint generic_button_border

