#       Faders
# --------------------------------------

# bg
--out gen_volbg_vert gen_panbg_vert                             --frame frame_fader_bg                      --images    fader_bg_fill  -tint generic_fader_bg           fader_bg_border -tint generic_fader_border
--out gen_volbg_horz gen_panbg_horz             --rotate 90     --frame frame_fader_bg -rotate 90 -flip-v   --images    fader_bg_fill  -tint generic_fader_bg           fader_bg_border -tint generic_fader_border

# bg - dark
--out gen_volbg_vert_dark gen_panbg_vert_dark                   --frame frame_fader_bg                      --images    fader_bg_fill  -tint generic_fader_dark_bg      fader_bg_border -tint generic_fader_dark_border
--out gen_volbg_horz_dark gen_panbg_horz_dark   --rotate 90     --frame frame_fader_bg -rotate 90 -flip-v   --images    fader_bg_fill  -tint generic_fader_dark_bg      fader_bg_border -tint generic_fader_dark_border

# thumb
--out gen_volthumb_vert                         --images    fader_thumb_l_fill              -tint generic_fader_thumb_bg            fader_thumb_l_border            -tint generic_fader_thumb_border        frame/frame_fader_l
--out gen_panthumb_vert                         --images    fader_thumb_fill                -tint generic_fader_thumb_bg            fader_thumb_border              -tint generic_fader_thumb_border        frame/frame_fader
--out gen_volthumb_horz         --rotate 270    --images    fader_thumb_l_fill      -flip-v -tint generic_fader_thumb_bg            fader_thumb_l_border  -flip-v   -tint generic_fader_thumb_border        frame/frame_fader_l
--out gen_panthumb_horz         --rotate 270    --images    fader_thumb_fill        -flip-v -tint generic_fader_thumb_bg            fader_thumb_border    -flip-v   -tint generic_fader_thumb_border        frame/frame_fader     -flip-v 

# thumb -dark
--out gen_volthumb_vert_dark                    --images    fader_thumb_l_fill              -tint generic_fader_dark_thumb_bg       fader_thumb_l_border            -tint generic_fader_dark_thumb_border   frame/frame_fader_l
--out gen_panthumb_vert_dark                    --images    fader_thumb_fill                -tint generic_fader_dark_thumb_bg       fader_thumb_border              -tint generic_fader_dark_thumb_border   frame/frame_fader
--out gen_volthumb_horz_dark    --rotate 270    --images    fader_thumb_l_fill      -flip-v -tint generic_fader_dark_thumb_bg       fader_thumb_l_border  -flip-v   -tint generic_fader_dark_thumb_border   frame/frame_fader_l
--out gen_panthumb_horz_dark    --rotate 270    --images    fader_thumb_fill        -flip-v -tint generic_fader_dark_thumb_bg       fader_thumb_border    -flip-v   -tint generic_fader_dark_thumb_border   frame/frame_fader     -flip-v 

#       Buttons - Large
# --------------------------------------

# off state
--out gen_home          --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     forward         -tint generic_button_symbol_off  -flip-h      border -tint generic_button_border
--out gen_end           --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     forward         -tint generic_button_symbol_off               border -tint generic_button_border
--out gen_pause         --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     pause           -tint generic_button_symbol_off               border -tint generic_button_border
--out gen_play          --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     play            -tint generic_button_symbol_off               border -tint generic_button_border
--out gen_repeat_off    --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     repeat          -tint generic_button_symbol_off               border -tint generic_button_border
--out gen_stop          --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_off     stop            -tint generic_button_symbol_off               border -tint generic_button_border

# on state
--out gen_pause_on      --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_on      pause           -tint generic_button_symbol_on                border -tint generic_button_border
--out gen_play_on       --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_on      play            -tint generic_button_symbol_on                border -tint generic_button_border
--out gen_repeat_on     --spritesheet --button --frame-make 0 0 1 1 --images bg -tint generic_button_bg_on      repeat          -tint generic_button_symbol_on                border -tint generic_button_border

