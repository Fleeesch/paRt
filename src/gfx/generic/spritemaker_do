--out gen_mute_off      --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint generic_button_bg_off       mute        -tint generic_button_symbol_off            border_small -tint generic_button_border
--out gen_solo_off      --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint generic_button_bg_off       solo        -tint generic_button_symbol_off            border_small -tint generic_button_border

