--out gen_env_read      --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_green         env_read        -tint button_symbol_on    border_small -tint generic_button_border
--out gen_env_latch     --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_yellow        env_latch       -tint button_symbol_on    border_small -tint generic_button_border
--out gen_env_preview   --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_yellow        env_preview     -tint button_symbol_on    border_small -tint generic_button_border
--out gen_env_touch     --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_violet        env_touch       -tint button_symbol_on    border_small -tint generic_button_border
--out gen_env_write     --spritesheet --button --frame-make 0 0 1 1 --images bg_small -tint color_red           env_write       -tint button_symbol_on    border_small -tint generic_button_border

